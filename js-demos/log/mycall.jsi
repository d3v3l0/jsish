function mycall(args:array=void, conf:object=void) {
    var options = { Debug: false, Trace: false };
    var self = { cnt:0 };
    moduleOpts(options, self, conf);
    
    LogDebug("MYLOC 1:", args.join(','));
    LogTrace("MYLOC 2:", args.join(','));
}

LogDebug("MYGLOB 1");
LogTrace("MYGLOB 2");

provide(mycall);
puts('mycall loaded');

if (isMain())
    runModule(mycall);

